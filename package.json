{
  "name": "antd-intl-example",
  "private": true,
  "version": "1.0.0",
  "entry": {
    "en-US": "./src/entry/en-US.js",
    "zh-Hans-CN": "./src/entry/zh-Hans-CN.js",
    "index": "./src/entry/index.jsx"
  },
  "dependencies": {
    "antd": "0.11.x",
    "atool-build": "0.4.x",
    "babel-plugin-antd": "^0.1.2",
    "react": "~0.14.3",
    "react-dom": "~0.14.3",
    "react-intl": "^2.0.0-beta-2"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-eslint": "~4.0.5",
    "babel-plugin-react-intl": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "dora": "^0.2.x",
    "dora-plugin-atool-build": "0.4.x",
    "dora-plugin-proxy": "^0.5.2",
    "eslint": "~1.6.0",
    "eslint-config-airbnb": "~0.1.0",
    "eslint-plugin-react": "~3.5.1",
    "pre-commit": "1.x",
    "rimraf": "^2.5.0"
  },
  "pre-commit": [
    "lint"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "dora -p 8001 --plugins atool-build,proxy",
    "lint": "eslint --ext .js,.jsx src",
    "build": "atool-build -o ./dist/${npm_package_name}/${npm_package_version}/",
    "build:i18n-messages": "rimraf i18n-messages && BABEL_ENV=i18n babel --quiet src > /dev/null",
    "build:i18n-js": "npm run build:i18n-messages && babel-node --presets es2015 scripts/translate.js"
  }
}
